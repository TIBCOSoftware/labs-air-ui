    "serve_au": "ng serve --proxy-config proxy.conf.prod.au.json --ssl true --source-map --aot",
    "build_prod_hash": "ng build --prod --base-href /webresource/apps/IoTCloudStarter/index.html --deploy-url /webresource/apps/IoTCloudStarter/ && cd ./dist/IoTCloudStarter/ && zip -r ./app.zip ./* && cd ../..",
