    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng-high-memory-serve-us": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.prod.us.json --ssl true --source-map --aot --host 0.0.0.0 --disable-host-check",
