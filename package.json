  "scripts": {
    "setup_minikube": "node scripts/setup.js minikube",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng-high-memory-serve-us-insec": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.prod.us.js --source-map --aot --host 0.0.0.0 --disable-host-check",
    "ng-high-memory-serve-us": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.prod.us.js --ssl true --source-map --aot --host 0.0.0.0 --disable-host-check",
    "ng-high-memory-build-prod": "node --max_old_space_size=8000 ./node_modules/@angular/cli/bin/ng build --prod --base-href /webresource/apps/ProjectAir/ --deploy-url /webresource/apps/ProjectAir/ && cd ./dist/ProjectAir/ && zip -r ./app.zip ./* && cd ../..",
    "serve_eu": "ng serve --proxy-config proxy.conf.prod.eu.js --ssl true --source-map --aot",
    "serve_us": "ng serve --proxy-config proxy.conf.prod.us.js --ssl true --source-map --aot",
    "serve_au": "ng serve --proxy-config proxy.conf.prod.au.js --ssl true --source-map --aot",
    "serve_eu_cont": "ng serve --proxy-config proxy.conf.prod.eu.js --ssl true --source-map --aot --host 0.0.0.0",
    "serve_us_cont": "ng serve --proxy-config proxy.conf.prod.us.js --ssl true --source-map --aot --host 0.0.0.0",
    "serve_au_cont": "ng serve --proxy-config proxy.conf.prod.au.js --ssl true --source-map --aot --host 0.0.0.0",
    "build_prod_hash": "ng build --prod --base-href /webresource/apps/ProjectAir/index.html --deploy-url /webresource/apps/ProjectAir/ && cd ./dist/ProjectAir/ && zip -r ./app.zip ./* && cd ../..",
    "build_prod": "ng build --prod --base-href /webresource/apps/ProjectAir/ --deploy-url /webresource/apps/ProjectAir/ && cd ./dist/ProjectAir/ && zip -r ./app.zip ./* && cd ../.."
  },
